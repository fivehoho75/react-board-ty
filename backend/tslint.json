{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": ["node_modules", "*.config.js", "*.json", "*.js"]
  },
  "jsRules": {},
  "rules": {
    "member-access": false,
    "prettier": true,
    "interface-name": [true, "never-prefix"],
    "object-literal-sort-keys": false
  },
  "rulesDirectory": []
}
